<%= simple_form_for(@menu) do |f| %>
  <%= f.input :location, collection: [[t('menu.info'), :info], [t('menu.voting'), :voting], [t('menu.contact'), :contact]] %>
  <%= f.input :index %>
  <%= f.input :name %>
  <%= f.input :link %>
  <%= f.input :visible %>
  <%= f.input :turbolinks %>
  <%= f.input :blank_p %>
  <%= f.button :submit, id: :menu_id %>
<% end %>
